<h1>Users#new</h1>
<p>Find me in app/views/users/new.html.erb</p>

<%= form_for User.new do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :email %>
    <%= f.email_field :email %>
    <%= f.label :birthday %>
    <%= f.date_field :birthday %> 
    <%= f.label :password %>
    <%= f.password_field :password %>
    <%= f.submit %>
<% end %>
